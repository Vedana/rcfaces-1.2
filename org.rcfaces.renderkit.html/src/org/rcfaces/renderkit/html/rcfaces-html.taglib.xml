<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE facelet-taglib PUBLIC "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN" "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib>
	<namespace>http://rcfaces.org/html</namespace>
	<tag>
		<tag-name>init</tag-name>
		<handler-class>
			org.rcfaces.renderkit.html.internal.facelets.InitHandler
		</handler-class>
	</tag>
	<tag>
		<tag-name>cssStyle</tag-name>
		<handler-class>
			org.rcfaces.renderkit.html.internal.facelets.CssStyleHandler
		</handler-class>
	</tag>
	<tag>
		<tag-name>loadClientBundle</tag-name>
		<handler-class>
			org.rcfaces.renderkit.html.internal.facelets.LoadClientBundleHandler
		</handler-class>
	</tag>
	<tag>
		<tag-name>javaScript</tag-name>
		<handler-class>
			org.rcfaces.renderkit.html.internal.facelets.JavaScriptHandler
		</handler-class>
	</tag>
</facelet-taglib>
